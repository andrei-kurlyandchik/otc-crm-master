}

.tabs-claims {
	width 100%
	position relative
	.btn-dd {
		position absolute
		top: 0px
		right 0px
	}
	&__date {
		font-size 16px
		line-height 27px
		font-weight 500
		color #333333
		margin-bottom 10px
	}
	.tabs-claims__table {
		width 100%
		td {
			padding 0 20px 10px 0
		}
	}
}

.items-order {
	border-top: 1px solid #ccc;
	&:first-child {
		border none
	}
	&__title {
		font-size 16px
		line-height 27px
		font-weight 500
		color #333333
		margin-bottom 25px
	}
	&__content {
		display flex
		justify-content flex-start
		margin-bottom 25px
	}
	&__img {
		width 75px
		margin-right 25px
		img {
			width 100%
		}
	}
	&__info {
		padding-right 20px
		flex 1
		width 0
		&--title {
			font-size 16px
			line-height 27px
			font-weight 500
			color #333333
		}
		&--list {
			display inline-flex
		}
		&--list-content {
			font-size 16px
			line-height 27px
			font-weight 300
			color #333333
			margin-right 25px
		}
	}
	&__price {
		width 120px
		padding-right 20px
		padding-top 4px
	}
	&__btn {
		width 270px
		display flex
		flex-flow column
		.btn-dd {
			justify-content center
		}
		&--refund {
			display flex
			align-items center
			justify-content space-between
			font-size 16px
			line-height 27px
			font-weight 500
			color #136C00
			margin-bottom 12px
		}
	}
}

.container-tabs-wrapper {
	width 100%
}

.tabs-notes {
	.title-h2 {
		margin-bottom 25px
	}
	.title-h2 span {
		font-size 16px
		line-height 18px
		font-weight 400
		color #3A3A3A
		margin-left 10px
	}

	.btn-wrapper {
		margin-bottom 16px
	}
}

.tabs-issues {
	.title-h2 {
		margin-bottom 25px
	}
	.title-h2 span {
		font-size 16px
		line-height 18px
		font-weight 400
		color #3A3A3A
		margin-left 10px
	}
	.btn-wrapper {
		margin-bottom 35px
	}
}

.tabs-order {
	margin-bottom 20px
}

.tabs-order-wrapper {
	display flex
	align-items center
	justify-content space-between
	margin-top 25px
}

.tabs-history {
	.form__full {
		border-bottom 1px solid #ccc
	}
	.history-calendar-data, .history-calendar-card {
		margin-left 70px
	}
}

.tabs-history-tags {
	display flex
	flex-wrap wrap
	padding 20px 20px 0px
}

.history-calendar {
	width 100%
}

.history-calendar-data {
	position relative
	width 100%
	padding 40px 0 50px 44px
	font-size 24px
	font-weight 500
	color #333333
	display block
	&:before {
		content ''
		position absolute
		top 0px
		left 12px
		width 2px
		height 100%
		background-color #D8D8D8
	}
}

.history-calendar-card {
	position relative
	padding 0 15px 50px 44px
	&:before {
		content ''
		position absolute
		top 0px
		left 12px
		width 2px
		height 100%
		background-color #D8D8D8
	}
	&__circle {
		width 24px
		height 24px
		position absolute
		top 0px
		left 1px
		background #FFFFFF
		box-shadow 0 2px 1px -1px rgba(0,0,0,0.20), 0 1px 1px 0 rgba(0,0,0,0.12), 0 2px 1px -1px rgba(0,0,0,0.14)
		border-radius 12px
		&:before {
			content ''
			position absolute
			width 16px
			height 16px
			background-color #9E9E9E
			top 50%
			left 50%
			transform: translate(-50%, -50%);
			border-radius 50%
		}
	}
	&-content {
		display flex
		justify-content space-between
	}
	&__content {
		text-align left
		//width 550px
		flex 1
		&--title {
			font-size 16px
			line-height 27px
			font-weight 500
			color #333333
		}
		&--content {

		}
	}
	&__data {
		text-align right
		//width 200px
		flex 0.5
	}
	.tags-history {
		display inline-flex
	}
}

.making-new-order__header {
	margin 0 -15px
	position relative
	display flex
	justify-content space-between
	.link-back {
		font-size 16px
		font-weight 300
		color #0F73E7
		text-decoration underline
		position absolute
		top 0px
		left 0px
	}
	.title-h2 {
		padding-top 55px
	}
}

.making-new-order__content {
	display flex
	align-items flex-start
	justify-content space-between
	margin 0 -15px 20px
}

.making-new-order {
	flex 1
	width 0
	position relative
	margin 0 auto
	//padding-right 20px
	padding-right 355px
	.tabs__caption {
		padding-top 15px
	}
	.title-h3 {
		padding 20px 30px 0 30px
	}
}

.card-box {
	width 340px
	.panel__outer {
		width 100%
		height 100%
		padding 25px 0
	}
	.title-h2 {
		margin-bottom 15px
		padding 0 17px
		span {
			font-size 16px
			line-height 19px
			font-weight 300
			color #3A3A3A
			margin-left 10px
		}
		&.margin-top {
			margin-top 27px
		}
	}
	.title-p {
		padding 0 17px 17px
		margin-bottom 0
		border-bottom 1px solid #ccc
	}
}

.card-box__big {
	display block
	position fixed
	top 0
	right 15px
	height: 100%
	padding-top: 120px
	&.not-product {
		.panel__outer {
			height auto
		}
		.summary-panel {
			position relative
		}
	}
}

.making-order-list {
	width 100%
	border-top 1px solid #ccc
	margin-top 20px
}

.making-order-info {
	display block
	padding 23px 23px 23px 0
	background-color white
	border-bottom 1px solid #ccc
	position relative
	&.gray {
		background-color #d0d8dc
	}
	&.red {
		background #ffebee
	}
	&:firs-child {
		border-bottom 0
	}
	&__img {
		width 80px
		margin-right 15px
		margin-top 5px
		flex 0 0 auto
		img {
			width 100%
		}
	}
	&__row {
		display flex
		flex-flow column
		width 100%
	}
	&__label {
		position absolute
		top 0
		left 0
		font-size 12px
		line-height 14px
		font-weight 300
		color white
		padding 2px 6px
		background-color #5E656C
		border-bottom-right-radius: 20px;
		border: 2px solid transparent;
		min-width: 400px;
	}
	&__col {
		display flex
		align-items center
		&.flex-col {
			flex-flow column
			align-items flex-end
		}
	}
	&__content {
		&--title {
			font-size 16px
			line-height 27px
			font-weight 500
			color #333333
			margin-bottom 7px
		}
		&--desc {
			font-size 12px
			font-weight 300
			color #5D656D
			span {
				font-weight 500
				color #333333
			}
		}
	}
	&__box {
		margin-bottom 8px
		display flex
		width 100%
		.title {
			width 90px
			margin-right 10px
			font-size 14px
			line-height 16px
			font-weight 300
			color #3A3A3A
			flex 0 0 auto
		}
		.text {
			font-size 14px
			line-height 16px
			font-weight 300
			color #3A3A3A
		}
	}
	&__box--show {
		display none
		&.show {
			display block
		}
	}
	&__price {
		font-size 22px
		font-weight 500
		line-height 27px
		color #333333
		&.dis {
			opacity .5
		}
	}
	&__items {
		font-size 12px
		font-weight 300
		color #5D656D
		margin-bottom 8px
	}
	&__out {
		font-size 14px
		line-height 16px
		font-weight 300
		color #333333
		text-transform uppercase
	}
	&__disc {
		font-size 14px
		line-height 16px
		font-weight 300
		color #D0021B
		text-transform uppercase
	}
	&__btn {
		display flex
		align-items center
		justify-content flex-end
		margin-top 7px
	}
	.btn-link {
		height 40px
		display flex
		align-items center
		justify-content center
		font-size 14px
		font-weight 400
		color #0F73E7
		padding 0 15px
		border-radius 4px
		margin-left 20px
		text-transform uppercase
		cursor pointer
	}
