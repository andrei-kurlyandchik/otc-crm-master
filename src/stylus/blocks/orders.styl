.issue-info-popup__dis {
	width 160px
	font-size 16px
	line-height 18px
	font-weight 400
	color: #3A3A3A;
	flex 1
	display flex
	a {
		color: #0f73e7;
		text-decoration: underline;
	}
   	.tags--red {
		height 33px
		margin-bottom 0
