.issues-card-tags {
	position: absolute;
	right: 0;
	top: 15px;
	display: flex;
}

.issues-card-wrapper {
	display flex
	justify-content space-between
	margin-top 20px
}

.issues-card-col {
	flex 1
	display flex
}

.issues-card-col__small {
	flex 1
}

.issues-card-col__big {
	padding-left 15px
	.issues-card-col__title {
		width 225px
	}
}

.issues-card-row {
	display flex
	margin-bottom 10px

}

.issues-card-col__title {
	font-size 14px
	line-height 16px
	font-weight 500
	color: #3A3A3A;
	width 140px
}

.issues-card-col__dis {
	font-size 14px
	line-height 16px
	font-weight 400
	color: #3A3A3A;
	a {
		color: #0f73e7;
		text-decoration: underline;
	}
}

.issue-info-popup {
	width 100%
	border-bottom: 1px solid #cecece;
	padding-bottom 20px
	margin-bottom 30px
}

.issue-info-popup__row {
	display flex
	margin-bottom 10px
	flex: 1;
}

.issue-info-popup__title {
	width 160px
	font-size 16px
	line-height 18px
	font-weight 400
	color: #3A3A3A;
