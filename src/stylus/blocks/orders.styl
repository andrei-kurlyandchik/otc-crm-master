	padding 15px 17px
	border-bottom 1px solid #CCCCCC
	&:last-child {
		border-bottom 0
	}
	.btn-del {
		position absolute
		top 14px
		right 10px
	}
	.btn-fav {
		position absolute
		top 14px
		right 40px
	}
	&__title {
		font-size 16px
		font-weight 300
		color #3A3A3A
		margin-bottom 10px
		padding-right 55px
	}
	&__content {
		display flex
		flex-flow column
		margin-bottom 14px
		&--name {
			font-size 16px
			font-weight 300
			color #3A3A3A
		}
		&--price {
			font-size 16px
			font-weight 300
			color #3A3A3A
		}
	}
	&__footer {
		display flex
		align-items flex-end
		justify-content space-between
		&--save {
			font-size 16px
			font-weight 300
			color #0F73E7
			text-decoration underline
			text-align left
		}
		&--price {
			font-size 24px
