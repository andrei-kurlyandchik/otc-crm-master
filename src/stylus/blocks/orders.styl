.orders-summary-header {
	display flex
	align-items center
	justify-content space-between
	margin-bottom 20px
	.link {
		color #0f73e7
		text-decoration underline
	}
	.btn-link {
		padding 30px 35px
		border-radius 20px
		font-size 18px
		font-weight: 500
		color #0F73E7
		background-color #fff
		box-shadow 0 2px 2px 0 rgba(56,142,200,0.30)
	}
}

.order-summer-content {
	border-radius 20px
	background-color #fff
}

.order-summer-info {
	padding 23px 30px
	border-radius 20px
	background-color #F8FAFC
	margin-bottom 30px
