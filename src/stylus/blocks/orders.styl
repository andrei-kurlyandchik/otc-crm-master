.tabs-search-title {
	display flex
	justify-content space-between
	margin-bottom 25px
	&__col {
		display flex
		align-items flex-end
	}
	.title-h3 {
		font-size 20px
		font-weight 500
		padding 0
	}
	.btn-calls-filters {
		font-size 20px
		font-weight 300
		margin-left 25px
		text-decoration underline
		color #0F73E7
		cursor pointer
	}
}

@media screen and (max-width: 1880px) {
	.issues-card-wrapper {
		flex-flow column
	}

	.issues-card-col__big {
		padding-left 0
		margin-top 20px
	}
}

@media screen and (max-width: 1600px) {
	.summary-small {
		display block
	}
	.card-box__big {
		display none
	}
	.making-new-order {
		padding-right 0
	}
}