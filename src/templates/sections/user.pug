section.panel.panel--user-info
    .container
        .panel__row.margin-null
            .panel__col.panel__col--left.panel__col--pad
                .panel__outer.panel--user.panel--outdated
                    .find-user
                        .find-user__title Find user by Member ID
                        label.form__label
                            span.form__ttl Member ID
                            input.form__inp(type='text' value='' placeholder='615246')
                .panel__outer.panel--user.panel--active
                    .panel__itm
                        .user
                            div.user__label.user__label--active Active user
                            p.title-h2 Edward Green
                            div.user-profile-tags
                                div.user-profile-tags__list
                                    .tags.tags--blue Do not call
                                    .tags.tags--blue Talking loud
                            table.user__table
                                tr
                                    td Gender
                                    td Male
                                tr
                                    td Language
                                    td
                                        .user__row
                                            .user__col
